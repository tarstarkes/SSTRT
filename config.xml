<?xml version='1.0' encoding='utf-8'?>
<widget id="com.connarstone.sstrt" version="1.1.5" versionCode="14" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>SSTRT</name>
    <description>Start Stop Team Relay Timer records contestant times for team relay running. Initially built for the benefit of the Notheastern Oregon Elkhorn Relay.</description>
    <author email="stonec2@eou.edu" href="https://github.com/tarstarkes">Connar Stone</author>
    <feature name="http://api.phonegap.com/1.0/file" />
    <feature name="http://api.phonegap.com/1.0/media" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="ios-statusbarstyle" value="default" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="android-installLocation" value="auto" />
    <preference name="DisallowOverscroll" value="true" />
    <splash height="480" platform="ios" src="Default.png" width="320" />
    <splash height="960" platform="ios" src="Default@2x.png" width="640" />
    <splash height="1136" platform="ios" src="Default-568h@2x.png" width="640" />
    <splash height="1334" platform="ios" src="Default-667h@2x.png" width="750" />
    <splash height="2208" platform="ios" src="Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1024" platform="ios" src="Default-Portrait.png" width="768" />
    <splash height="2048" platform="ios" src="Default-Portrait@2x.png" width="1536" />
    <platform name="ios">
        <icon height="180" src="icon-60@3x.png" width="180" />
        <icon height="60" src="icon-60.png" width="60" />
        <icon height="120" src="icon-60@2x.png" width="120" />
        <icon height="76" src="icon-76.png" width="76" />
        <icon height="152" src="icon-76@2x.png" width="152" />
        <icon height="167" src="icon-83.5@2x.png" width="167" />
        <icon height="29" src="icon-small.png" width="29" />
        <icon height="58" src="icon-small@2x.png" width="58" />
        <icon height="87" src="icon-small@3x.png" width="87" />
        <icon height="40" src="icon-40.png" width="40" />
        <icon height="80" src="icon-40@2x.png" width="80" />
        <icon height="120" src="icon-40@3x.png" width="120" />

        <!-- iPhone / iPod Touch -->
        <icon src="icon.png" width="57" height="57" />
        <icon src="icon@2x.png" width="114" height="114" />

        <!-- iPad -->
        <icon src="icon-72.png" width="72" height="72" />
        <icon src="icon-72@2x.png" width="144" height="144" />

        <!-- iPad Spotlight and Settings Icon -->
        <icon src="icon-50.png" width="50" height="50" />
        <icon src="icon-50@2x.png" width="100" height="100"/>
    </platform>
    <platform name="android">
        <icon qualifier="ldpi" src="ldpi.png" />
        <icon qualifier="mdpi" src="mdpi.png" />
        <icon qualifier="hdpi" src="hdpi.png" />
        <icon qualifier="xhdpi" src="xhdpi.png" />
        <icon qualifier="xxhdpi" src="xxhdpi.png" />
        <icon qualifier="xxxhdpi" src="xxxhdpi.png" />
    </platform>
    <platform name="winphone">
    </platform>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="cordova-plugin-file" spec="~6.0.2" />
    <!-- <plugin name="cordova-plugin-file" spec="~4.3.3" /> -->
    <!-- <plugin name="cordova-plugin-file-transfer" /> -->
    <plugin name="cordova-plugin-statusbar" spec="~2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />

    <!-- Android Preferences -->
    <preference name="android-targetSdkVersion" value="28" />
</widget>
